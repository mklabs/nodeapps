<div class="lint">
  <a class="giturl" href="<%= filelint.giturl %>">github file</a>
  <table>
    <thead>
      <tr>
        <th width="1%">Line</th>
        <th>Reason</th>
        <th width="60%">Evidence</th>
      </tr>
    </thead>
    <tbody>
      <% for (var key in filelint.lints) { if (!filelint.lints[key]) continue; %>
        <tr>
          <td><a href="<%= filelint.giturl %>#L<%= filelint.lints[key].line %>"><%= filelint.lints[key].line %></a></td>
          <td><%= filelint.lints[key].reason %></td>
          <td><%= filelint.lints[key].evidence %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
  
  <div class="frame source">
    <table cellpadding="0" cellspacing="0">
      <tbody>
        <tr>
          <td>
            <pre class="line_numbers">
              <% for(var i = 1; i <= filelint.contentLength; i++) { %> <span id="LID<%=i%>" rel="#L<%=i%>"><%=i%></span> <% } %>
            </pre>
          </td>
          <td width="100%">
            <pre><code><%-filelint.content%></code></pre>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>